type TaxPayer = 
 record {
   address: text;
   firstName: text;
   image: opt text;
   lastName: text;
   tid: nat;
 };
type Result = 
 variant {
   err: text;
   ok: nat;
 };
service : {
  addTaxPayer: (text, text, text, opt text) -> (Result);
  getAllTaxPayers: () -> (vec TaxPayer) query;
  getTaxPayerByTID: (nat) -> (opt TaxPayer) query;
}
